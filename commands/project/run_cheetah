# }

python ./main/run/full.py -- @CUSTOM=CHEETAH_BASELINE experiment_name:sam_baseline_2 coach_settings:enable_sam:true 2>&1 | tee logs/cheetah/baseline_2.log && {
    python ./main/run/full.py -- @CUSTOM=CHEETAH_BASELINE               experiment_name:sam_baseline_3                    coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_baseline_3.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_Q_PLUS_STATE           experiment_name:sam_q_plus_state_loss_1           coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_q_plus_state_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_Q_UNFAVORED_PLUS_STATE experiment_name:sam_q_unfavored_plus_state_loss_1 coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_q_unfavored_plus_state_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_Q_IGNORED_PLUS_STATE   experiment_name:sam_q_ignored_plus_state_loss_1   coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_q_ignored_plus_state_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_Q                      experiment_name:sam_q_loss_1                      coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_q_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_CONSISTENT_COACH       experiment_name:sam_consistent_coach_loss_1       coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_consistent_coach_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_CONSISTENT_VALUE       experiment_name:sam_consistent_value_loss_1       coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_consistent_value_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_HALF_CONSISTENT_COACH  experiment_name:sam_half_consistent_coach_loss_1  coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_half_consistent_coach_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_STATE_TRIPLE           experiment_name:sam_state_triple_1                coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_state_triple_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_DELAYED_COACH_LOSS     experiment_name:sam_delayed_coach_loss_1          coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_delayed_coach_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_HALF_CONSISTENT_COACH  experiment_name:sam_half_consistent_coach_loss_1  coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_half_consistent_coach_loss_1.log 
    python ./main/run/full.py -- @CUSTOM=CHEETAH_DELAYED_COACH          experiment_name:sam_delayed_coach_loss_1          coach_settings:enable_sam:true "$@" &> logs/cheetah/sam_delayed_coach_loss_1.log 
}